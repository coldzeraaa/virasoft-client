{
  "name": "virasoft-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:cache": "rm -rf .next/cache",

    "dev:virasoft:prod": "npm run clean:cache &&  APP_INDEX=virasoft next dev -p 3202",
    "dev:virasoft:windows:prod": "npm run clean:cache && cross-env APP_INDEX=virasoft next dev -p 3202",
    "dev:virasoft:local": "npm run clean:cache && STAGE=local APP_INDEX=virasoft next dev -p 3203",
    "dev:virasoft:docker": "npm run clean:cache && STAGE=docker APP_INDEX=virasoft next dev -p 3204",
    "build:virasoft": "APP_INDEX=virasoft next build",
    "build:virasoft:windows": "cross-env APP_INDEX=virasoft next build",

    "dev:goldenoil:prod": "npm run clean:cache &&  APP_INDEX=goldenoil next dev -p 3212",
    "dev:goldenoil:windows:prod": "npm run clean:cache && cross-env APP_INDEX=goldenoil next dev -p 3212",
    "dev:goldenoil:local": "npm run clean:cache && STAGE=local APP_INDEX=goldenoil next dev -p 3213",
    "dev:goldenoil:docker": "npm run clean:cache && STAGE=docker APP_INDEX=goldenoil next dev -p 3214",
    "build:goldenoil": "APP_INDEX=goldenoil next build",
    "build:goldenoil:windows": "cross-env APP_INDEX=goldenoil next build",

    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:docker": "graphql-codegen --config codegen.docker.yml",
    "codegen:local": "graphql-codegen --config codegen.local.yml"
  },
  "dependencies": {
    "@apollo/experimental-nextjs-app-support": "^0.11.7",
    "@heroicons/react": "^2.1.1",
    "@react-google-maps/api": "^2.20.5",
    "@searchkit/api": "^4.13.0",
    "@searchkit/instantsearch-client": "^4.14.1",
    "apollo-upload-client": "^18.0.1",
    "field-form": "^1.2.10",
    "holy-loader": "^2.3.11",
    "js-cookie": "^3.0.5",
    "ks-instantsearch-routing": "^1.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.471.0",
    "next": "14.2.17",
    "nextjs-middleware-client": "^1.0.12",
    "rc-field-form": "^2.7.0",
    "react": "^18",
    "react-animate-height": "^3.2.3",
    "react-dom": "^18",
    "react-instantsearch-nextjs": "^0.3.20",
    "react-instantsearch-router-nextjs": "^7.13.9",
    "react-toastify": "^10.0.6",
    "simple-helper-fns": "^1.0.7",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.2.4",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@munkhsukhj/eslint-config-mjts": "^1.0.10",
    "@tailwindcss/typography": "^0.5.15",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/qrcode": "^1.5.5",
    "cross-env": "^7.0.3",
    "daisyui": "^4.10.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.17",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
